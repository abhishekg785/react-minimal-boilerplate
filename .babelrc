    "presets": [
        ["@babel/preset-env", {
            "modules": false,
            "useBuiltIns": "entry",
            "loose": true,
        }],
        "@babel/preset-flow",
        ["@babel/preset-react", { "useBuiltIns": true }]
    ],
    "plugins": [
        ["@babel/plugin-proposal-class-properties", { "loose": true }]
    ]
}